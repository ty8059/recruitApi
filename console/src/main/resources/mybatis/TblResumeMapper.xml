            </if>
            <if test="arrivalTime != null">
                #{arrivalTime,jdbcType=VARCHAR},
            </if>
            <if test="reserved1 != null">
                #{reserved1,jdbcType=VARCHAR},
            </if>
            <if test="reserved2 != null">
                #{reserved2,jdbcType=VARCHAR},
            </if>
            <if test="reserved3 != null">
                #{reserved3,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>

    <select id="findByPosition" resultType="com.admn.console.model.TblResume" resultMap="BaseResultMap">
        select
        <include refid="Union_Column_List"/>
        from TBL_RESUME t1
        left join TBL_EDU_EXP t2
        on t1.USER_ID = t2.USER_ID
        where 1 = 1
        <if test="position != null and position != ''">
            and t1.POSITION like CONCAT('%', #{position}, '%')
        </if>
        order by t1.RESUME_ID asc
    </select>
</mapper>